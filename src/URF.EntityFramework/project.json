{
  "packOptions": {
    "summary": "Unit of Work & (extensible/generic) Repositories Framework",
    "tags": [ "repository", "unitofwork", "pattern", "entity framework" ],
    "owners": [ "dopare" ],
    "projectUrl": "https://github.com/dopare/URF.NET",
    "licenseUrl": "https://github.com/dopare/URF.NET/blob/master/License.txt",
    "repository": {
      "type": "git",
      "url": "https://github.com/dopare/URF.NET"
    }
  },
  "authors": [ "Mkrtich Mazmanyan" ],
  "description": "This framework minimizes the surface area of your ORM technlogy from disseminating in your application. Framework provides an elegant way to implement a reusable and extensible Unit of Work and Repository pattern.",
  "version": "1.0.1-*",
  "title": "URF Entity Framework",
  "copyright": "Copyright (c) 2016 Mkrtich Mazmanyan",
  "dependencies": {
    "URF.Abstractions": {
      "version": "1.0.0-*",
      "target": "project"
    }
  },
  "buildOptions": {
    "warningsAsErrors": true
  },
  "frameworks": {
    "net45": {
      "frameworkAssemblies": {
        "System.Data": "4.0.0.0",
        "System.ComponentModel.DataAnnotations": "4.0.0.0"
      },
      "dependencies": {
        "EntityFramework": "6.1.3",
        "LinqKit": "1.1.3.1",
        "CommonServiceLocator": "1.3.0"
      }
    },
    "net451": {
      "frameworkAssemblies": {
        "System.Data": "4.0.0.0",
        "System.ComponentModel.DataAnnotations": "4.0.0.0"
      },
      "dependencies": {
        "EntityFramework": "6.1.3",
        "LinqKit": "1.1.3.1",
        "CommonServiceLocator": "1.3.0"
      }
    },
    "netstandard1.3": {
      "imports": [ "portable-net45+win8" ],
      "buildOptions": {
        "define": [ "COREFX" ]
      },
      "dependencies": {
        "NETStandard.Library": "1.5.0-rc2-24027",
        "System.ComponentModel.Annotations": "4.1.0-rc2-24027",
        "System.Data.Common": "4.0.1-rc2-24027",
        "System.Linq.Expressions": "4.0.11-rc2-24027",
        "Microsoft.EntityFrameworkCore": "1.0.0-rc2-final"
      }
    }
  }
}
